@model bookmarks.Models.User
@{
    ViewData["Title"] = "Register";
}

<form asp-action="register" asp-controller="account" method="POST">
  <div class="form-group">
    <label asp-for="Name">Name</label>
    <input type="text" class="form-control" asp-for="Name" value="Hello" placeholder="Name" readonly>
  </div>
  <div class="form-group">
    <label asp-for="Email">Email</label>
    <input type="email" class="form-control" asp-for="Email" value="Hello@@google.com" placeholder="Email">
  </div>
  <input type="hidden" asp-for="ProviderId" />
  <button type="submit" class="btn btn-primary">Submit</button>
</form>